#include <iostream>
#include <string>
#include <cmath>
#include <vector>

using namespace std;

int main(){
	int input, x, y, z, array[3] = { 0, 0, 0 };

	cin >> input;

	for (int i = 0; i < input; ++i){
		cin >> x >> y >> z;
		array[0] += x;
		array[1] += y;
		array[2] += z;
	}

	if (array[0] != 0 || array[1] != 0 || array[2] != 0)
		cout << "NO";
	else
		cout << "YES";

	return 0;
}
