#include <cstdlib>
#include <iostream>
#include <string>
#include "string.h"
using namespace std;

void Switch(char);

int main() 
{
    string temp;
    int num = temp.length();
    cin >> temp;
    for(int i = 0; i < temp.length(); i++){
         Switch(temp[i]); 
    }
        
    return 0;
}

void Switch(char c){
    
    switch(toupper(c)){
        case 'A':
            break;
        case 'E':
            break;
        case 'I':
            break;
        case 'U':
            break;
        case 'Y':
            break;
        case 'O':
            break;
        default:
            cout << "." << static_cast<char>(tolower(c));
            
    }
}
