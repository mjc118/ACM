using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    public class Program
    {
        public static void Main()
        {
            int num;

            num = Convert.ToInt32(Console.ReadLine());

            if (num < 3)
                Console.WriteLine("-1");
            else
            {
                Console.Write("3 2 1 ");
                for (int i = 4; i <= num; ++i)
                    if (i == num)
                        Console.Write(i);
                    else
                        Console.Write("{0} ", i);
            }                       
        }
    }
}
