#include <iostream>
#include <string>
#include <cmath>
#include <vector>

using namespace std;

int main(){
	string input1, input2;
	vector<int> ans;

	cin >> input1 >> input2;

	for (int i = 0; i < input1.length(); ++i){
		if (input1[i] != input2[i])
			ans.push_back(1);
		else
			ans.push_back(0);
	}

	for (auto i : ans)
		cout << i;

	return 0;
}
